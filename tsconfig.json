{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "dist",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
  },
  "include": [
    "./src/**/*",
  ],
  "exclude": ["node_modules", "**/__tests__/*"]
}
